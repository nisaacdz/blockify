{
  "name": "Rust Development Container",
  "image": "rust:latest",
  "customizations": {
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer",
        "serayuzgur.crates",
        "vadimcn.codelldb",
        "bungcip.better-toml",
        "gruntfuggly.todo-tree",
        "alexcvzz.sqlite",
        "florianklampfer.sqlite-viewer",
        "eamodio.gitlens",
        "alexander.error-lens"
      ],
      "settings": {
        "todo-tree.regex": "(\\/\\/|#|<!--|;|\\/\\*|^|^[ \\t]*(-|\\d+\\.))\\s*($TAGS)|todo!|unimplemented!"
      }
    }
  },
  "postCreateCommand": "rustup update",
  "features": {}
}
